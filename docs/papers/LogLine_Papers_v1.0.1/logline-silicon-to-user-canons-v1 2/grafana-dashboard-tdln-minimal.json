{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.5.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "bargauge",
      "name": "Bar gauge",
      "version": ""
    }
  ],
  "title": "TDLN Stack — Minimal Observability",
  "uid": "tdln-minimal-1769751946",
  "tags": [
    "logline",
    "tdln",
    "minimal"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "chip_id",
        "label": "Chip",
        "type": "query",
        "hide": 0,
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "query": "label_values(tdlne_decision_total, chip_id)",
        "current": {
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      },
      {
        "name": "slice",
        "label": "Slice",
        "type": "query",
        "hide": 0,
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "query": "label_values(tdlne_decision_total, slice)",
        "current": {
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "TDLN Evaluate Latency — p95",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(tdln_evaluate_latency_seconds_bucket{chip_id=~\"$chip_id\",slice=~\"$slice\"}[$__rate_interval])))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "TDLN Decisions — allow vs deny (rate)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (decision) (rate(tdlne_decision_total{chip_id=~\"$chip_id\",slice=~\"$slice\"}[$__rate_interval]))",
          "refId": "A",
          "legendFormat": "{{decision}}"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Deny % (last 5m)",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "(sum(rate(tdlne_decision_total{decision=\"deny\",chip_id=~\"$chip_id\",slice=~\"$slice\"}[5m])) / sum(rate(tdlne_decision_total{chip_id=~\"$chip_id\",slice=~\"$slice\"}[5m]))) * 100",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "timeseries",
      "title": "Registry writes by kind (rate)",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 6,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum by (entity_kind) (rate(registry_entity_write_total[$__rate_interval]))",
          "refId": "A",
          "legendFormat": "{{entity_kind}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Shield Upstream Latency — p95",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(shield_upstream_latency_seconds_bucket[$__rate_interval])))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Cold start (observado)",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "avg by (chip_id) (avg_over_time(chip_cold_start_seconds{chip_id=~\"$chip_id\",slice=~\"$slice\"}[$__interval]))",
          "refId": "A",
          "legendFormat": "{{chip_id}}"
        }
      ]
    }
  ]
}